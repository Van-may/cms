{
  "processors": [[
    "stylelint-processor-styled-components",
    {"parserPlugins": [
      "jsx",
      "objectRestSpread",
      "classProperties",
      "exportExtensions",
      "exportDefaultFrom"
    ]}
  ]],
  "extends": [
    "stylelint-config-recommended",
    "stylelint-config-styled-components"
  ],
  "options": {
    "parserPlugins": ["exportDefaultFrom", "classProperties", "objectRestSpread"]
  },
  "rules": {
    "block-no-empty": null,
    "no-duplicate-selectors": null,
    "selector-type-no-unknown": [true, {
      "ignoreTypes": ["$dummyValue"]
    }]
  }
}
